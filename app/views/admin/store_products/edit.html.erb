<h1>Edit Store Product</h1>
<div class="container-fluid">
	<div class="row">
		<div class="col-md-3"></div>
		<div class="col-md-6">
			<h2>Products</h2>
				<%= form_for @store_products, url: admin_store_product_path(@store_product.id), :html => { method: :patch } do |f| %>
					<%= label_tag(:store_product_name, "Store product name") %>
				  <%= @store_product.name %>
				<!-- <div class="form-group">
				  user can vote on product user, cannot vote on product
				</div> -->
				<div class="form-group">
					<%= f.submit "Edit", class: "btn btn-primary" %>
				</div>
				<% end %>
			</div>
		<div class="col-md-3"></div>
	</div>
</div>